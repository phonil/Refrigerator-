<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:orientation="vertical"
    android:visibility="visible">

    <LinearLayout
        android:id="@+id/findIdLinear1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="visible">

        <TextView
            android:id="@+id/findIdTitleTv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="30dp"
            android:text="Find ID"
            android:textColor="#FFFFFF"
            android:textSize="35dp" />


        <TextView
            android:id="@+id/nameTv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="23dp"
            android:text="Name"
            android:textColor="#6A6A6A" />

        <EditText
            android:id="@+id/nameET2"
            android:layout_width="350dp"
            android:layout_height="47dp"
            android:layout_marginLeft="30dp"
            android:background="#575758"
            android:backgroundTint="#262629"
            android:hint="User Name"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="#ffffff"
            android:textColorHint="#575758"
            android:textCursorDrawable="@null"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck,TextContrastCheck" />

        <TextView
            android:id="@+id/birthTv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="23dp"
            android:text="Birth"
            android:textColor="#6A6A6A" />

        <EditText
            android:id="@+id/birthEt2"
            android:layout_width="350dp"
            android:layout_height="47dp"
            android:layout_marginLeft="30dp"
            android:background="#575758"
            android:backgroundTint="#262629"
            android:hint="Your Birth"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="#ffffff"
            android:textColorHint="#575758"
            android:textCursorDrawable="@null"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck,TextContrastCheck" />

        <Button
            android:id="@+id/findIdBt1"
            android:layout_width="350dp"
            android:layout_height="58dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="25dp"
            android:backgroundTint="#CD475F"
            android:text="Find ID"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="17dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/findPwLinear1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="invisible">

        <TextView
            android:id="@+id/pwFindTv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="30dp"
            android:text="Reset Password"
            android:textColor="#FFFFFF"
            android:textSize="35dp" />


        <TextView
            android:id="@+id/idTv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="23dp"
            android:text="User ID"
            android:textColor="#6A6A6A" />

        <EditText
            android:id="@+id/idET3"
            android:layout_width="350dp"
            android:layout_height="47dp"
            android:layout_marginLeft="30dp"
            android:background="#575758"
            android:backgroundTint="#262629"
            android:hint="you@example.com"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="#ffffff"
            android:textColorHint="#575758"
            android:textCursorDrawable="@null"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck,TextContrastCheck" />

        <TextView
            android:id="@+id/nameTv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="23dp"
            android:text="Name"
            android:textColor="#6A6A6A" />

        <EditText
            android:id="@+id/nameEt3"
            android:layout_width="350dp"
            android:layout_height="47dp"
            android:layout_marginLeft="30dp"
            android:background="#575758"
            android:backgroundTint="#262629"
            android:hint="Your Name"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="#ffffff"
            android:textColorHint="#575758"
            android:textCursorDrawable="@null"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck,TextContrastCheck" />

        <TextView
            android:id="@+id/birthTv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="23dp"
            android:text="Birth"
            android:textColor="#6A6A6A" />

        <EditText
            android:id="@+id/birthEt3"
            android:layout_width="350dp"
            android:layout_height="47dp"
            android:layout_marginLeft="30dp"
            android:background="#575758"
            android:backgroundTint="#262629"
            android:hint="ex) 990101"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="#ffffff"
            android:textColorHint="#575758"
            android:textCursorDrawable="@null"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck,TextContrastCheck" />

        <Button
            android:id="@+id/resetBt1"
            android:layout_width="350dp"
            android:layout_height="58dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="25dp"
            android:backgroundTint="#CD475F"
            android:text="Reset Password"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="17dp" />

        <!-- New Password -->

        <TextView
            android:id="@+id/newPwTitleTv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="25dp"
            android:text="New Password"
            android:textColor="#FFFFFF"
            android:textSize="35dp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/newPwTv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="23dp"
            android:text="New Password"
            android:textColor="#6A6A6A"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/newPwEt1"
            android:layout_width="350dp"
            android:layout_height="47dp"
            android:layout_marginLeft="30dp"
            android:background="#575758"
            android:backgroundTint="#262629"
            android:hint="Password"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="#ffffff"
            android:textColorHint="#575758"
            android:textCursorDrawable="@null"
            android:visibility="invisible"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck,TextContrastCheck" />

        <TextView
            android:id="@+id/newPwCheckTv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="23dp"
            android:text="Check Password"
            android:textColor="#6A6A6A"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/newPwCheckEt1"
            android:layout_width="350dp"
            android:layout_height="47dp"
            android:layout_marginLeft="30dp"
            android:background="#575758"
            android:backgroundTint="#262629"
            android:hint="Password"
            android:inputType="textPersonName"
            android:padding="10dp"
            android:textColor="#ffffff"
            android:textColorHint="#575758"
            android:textCursorDrawable="@null"
            android:visibility="invisible"
            tools:ignore="TouchTargetSizeCheck,TouchTargetSizeCheck,TouchTargetSizeCheck,TextContrastCheck,TextContrastCheck" />

        <Button
            android:id="@+id/newPwOkBt1"
            android:layout_width="350dp"
            android:layout_height="58dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="25dp"
            android:backgroundTint="#CD475F"
            android:text="OK"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="17dp"
            android:visibility="invisible" />

    </LinearLayout>


</FrameLayout>